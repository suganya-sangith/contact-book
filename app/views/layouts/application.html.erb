<!DOCTYPE html>
<html>
<head>
  <title>Contactbook</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

	<div class="header">
		<div class="header-left">
			<div class="icon">
				<img src="/data/cb6.png" width = "50" height ="50" align="left"/>
			</div>
			<p>Contacts Book</p>
		</div>
		<div class="header-right">
			<ul>
				<li>
					<%=link_to_unless_current 'Home', root_path %> 
				</li>
				<li>
					<%=link_to_unless_current 'All', contacts_path %> 
				</li>
				<li>
					<%= link_to_unless_current 'Male', contacts_male_path %> 
				</li>
				<li>
					<%= link_to_unless_current 'Female', contacts_female_path %>
				</li>
			</ul>
			<div class="clear"></div>
		</div>
	</div>

	<%= yield %>

	<footer>
		<p> All text and design Â© 2014 Report Bee Edusys Pvt.Ltd.
			<a href="https://www.reportbee.com/">click here</a>
		</p>
	</footer>

</body>
</html>
